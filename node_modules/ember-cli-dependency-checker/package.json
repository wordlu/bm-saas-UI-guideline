{
  "name": "ember-cli-dependency-checker",
  "version": "2.2.1",
  "description": "Ember CLI addon for detecting missing npm and bower dependencies before executing ember commands",
  "keywords": [
    "bower",
    "check",
    "dependency",
    "ember",
    "ember-addon",
    "ember-cli",
    "node",
    "npm"
  ],
  "bugs": {
    "url": "https://github.com/quaertym/ember-cli-dependency-checker/issues"
  },
  "license": "MIT",
  "author": "Emre Unal",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quaertym/ember-cli-dependency-checker.git"
  },
  "scripts": {
    "test": "node tests/runner",
    "test:debug": "node debug tests/runner"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "find-yarn-workspace-root": "^1.1.0",
    "is-git-url": "^1.0.0",
    "resolve": "^1.5.0",
    "semver": "^5.3.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "ember-cli": "^2.16.2",
    "eslint-plugin-chai": "0.0.1",
    "eslint-plugin-mocha": "^4.8.0",
    "glob": "^7.1.1",
    "mocha": "^4.0.1",
    "mocha-eslint": "^4.1.0"
  },
  "engines": {
    "node": ">= 4"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "readmeFile": "README.md"
}
